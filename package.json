{
  "name": "intel-pipemanage",
  "version": "1.0.0",
  "description": "Intel Pipemanage",
  "main": "app.py",
  "private": true,
  "scripts": {
    "postinstall": "webpack --config webpack.prod.js",
    "log": "bash log.sh",
    "start": "bash start.sh",
    "run": "bash kill.sh && python3 app.py | tee dev_app.log",
    "restart": "bash restart.sh",
    "update": "bash update.sh",
    "ps": "bash ps.sh",
    "kill": "bash kill.sh",
    "dev": "concurrently --kill-others-on-fail --names 'BE,FE' --prefix-colors blue,green 'npm run start' 'webpack-dev-server --config webpack.dev.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nazihahmed/intel-pipemanage.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nazihahmed/intel-pipemanage/issues"
  },
  "homepage": "https://github.com/nazihahmed/intel-pipemanage#readme",
  "dependencies": {
    "semantic-ui-vue": "^0.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "concurrently": "^3.6.1",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  }
}
