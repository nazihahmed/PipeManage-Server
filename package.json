  "dependencies": {
    "jquery": "^3.3.1",
    "semantic-ui-vue": "^0.3.3",
    "socket.io-client": "^2.1.1",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.1",
