{
  "name": "intel-pipemanage",
  "version": "1.0.0",
  "description": "Intel Pipemanage",
  "main": "app.py",
  "private": true,
  "scripts": {
    "postinstall": "webpack --config webpack.prod.js",
    "log": "bash log.sh",
    "start": "bash start.sh",
    "app": "bash run.sh",
    "restart": "bash restart.sh",
    "update": "bash update.sh",
    "ps": "bash ps.sh",
    "kill": "bash kill.sh",
